<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.core.dao.ISequenceDao">
    <sql id="c_sequence">
        <!--最基础属性-->
        _sequence.name, _sequence.status, _sequence.logic_type logicType,
        _sequence.create_time createTime, _sequence.update_time updateTime
    </sql>
    <sql id="t_sequence">
        sequence
    </sql>
    <select id="selectById" parameterType="java.lang.Long" resultType="com.example.core.entity.ApplicationCase">
        select <include refid="c_sequence" />
        from <include refid="t_sequence"/> _sequence
        where id = #{id,jdbcType=BIGINT}
    </select>
